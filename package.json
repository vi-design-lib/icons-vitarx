{
  "name": "@vi-design/icons-vitarx",
  "version": "2.0.0",
  "description": "icons-vitarx是为vitarx框架打造的图标组件库。",
  "author": "ZhuChongLin <8210856@qq.com>",
  "keywords": [
    "vitarx",
    "icons-vitarx"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/icons.iife.js",
  "jsdelivr": "./dist/icons.iife.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./data": {
      "types": "./dist/assets/data.d.ts",
      "default": "./dist/assets/data.js"
    },
    "./unpkg": "./dist/icons.iife.js",
    "./jsdelivr": "./dist/icons.iife.js",
    "./icons/*": {
      "import": {
        "types": "./dist/widgets/icons/*.d.ts",
        "default": "./dist/widgets/icons/*.js"
      }
    }
  },
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && vite build && tsc && cp -r src/assets/iconfont.js ./dist/assets",
    "prepublishOnly": "npm run build",
    "push": "npm publish --access=public --registry https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "vitarx": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/types": "^19.8.1",
    "@types/node": "^22.10.2",
    "cheerio": "^1.0.0",
    "husky": "^9.1.7",
    "prettier": "^3.3.3",
    "sass-embedded": "^1.83.0",
    "typescript": "^5.7.2",
    "vite": "7.1.4"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ]
}
